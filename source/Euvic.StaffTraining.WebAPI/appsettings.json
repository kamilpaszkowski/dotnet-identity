{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Sql": "Server=(localdb)\\mssqllocaldb;Database=StaffTraining"
  },
  "Identity": {
    "AuthorizationUrl": "https://localhost:5002"
  }
}
